{
  "version": 2,
  "builds": [
    {
      "src": "/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "/js/app/admin/auth.js",
      "use": "@vercel/static"
    },
    {
      "src": "/js/app/admin/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "/js/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "npm install && npm run dev"
}